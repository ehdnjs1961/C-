#include <stdio.h>

int main()
{
	int a, b, c, count = 0;	
	scanf_s("%d", &count);
  
    for (int i = 1; i <= count; i++)
    { 
        scanf_s("%d%d%d", &a, &b, &c);
        printf("Scenario #%d:\n", i);
        if (a * a + b * b == c * c)
        {
            printf("yes\n");
        }
        else
        {
            printf("no\n");
        }
    }
	return 0;
}
